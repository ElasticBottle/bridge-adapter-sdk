{
  "name": "@elasticbottle/react-bridge-adapter-sdk",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup src/index.tsx --env.ESM_ONLY true --external react",
    "build-packages": "pnpm build",
    "dev": "tsup src/index.tsx --env.ESM_ONLY true --external react --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@elasticbottle/bridge-adapter-sdk-tsconfig": "^0.0.0",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.29.0",
    "eslint-config-bridge-adapter-sdk": "^0.0.0",
    "react": "^18.2.0",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@elasticbottle/core-bridge-adapter-sdk": "^0.0.0",
    "@tanstack/react-query": "^4.29.15",
    "lucide-react": "^0.252.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
